{
    "name": "haganjones/laravel-viewables",
    "type": "library",
    "description": "Class based approach to handling views in Laravel.",
    "keywords": ["laravel"],
    "homepage": "https://github.com/haganjones/laravel-viewables",
    "license": "MIT",
    "autoload": {
        "psr-4": { "HaganJones\\LaravelViewables\\": "app" },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "authors": [
        {
            "name": "Sam Jones",
            "email": "sam@haganjones.com",
            "homepage": "https://haganjones.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "3.3.*",
        "mockery/mockery": "0.9.*",
        "orchestra/database": "3.3.*"
    }
}
